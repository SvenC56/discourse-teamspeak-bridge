{
  "name": "discourse-teamspeak-bridge-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "nodemon --exec vue-cli-service serve",
    "docker": "docker build . -t app && docker run -d -p 8080:8080 app",
    "prettier": "prettier --config .prettierrc --write ./src/**/*.vue ./**/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SvenC56/discourse-teamspeak-bridge.git"
  },
  "author": "Sven Cieslok <mail@svencieslok.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SvenC56/discourse-teamspeak-bridge/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "eslint-teamcity": {
    "reporter": "inspections",
    "report-name": "My ESLint Violations",
    "error-statistics-name": "My ESLint Error Count",
    "warning-statistics-name": "My ESLint Warning Count"
  },
  "homepage": "https://github.com/SvenC56/discourse-teamspeak-bridge#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-moment": "^4.1.0",
    "vuetify": "^2.2.26"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^4.3.1",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-docker": "^0.2.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-masonry-css": "^1.0.3",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  }
}
